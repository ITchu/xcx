<!--pages/course/course.wxml-->
<view class='course-page'>
  <view class='contentview' wx:for="{{contentArr}}" wx:key="index" >
      <view class='titleView'>{{item.title}}</view>
      <view class='contenttext' wx:for="{{item.content}}" wx:key="index">
        <view class='contenttitle'>{{item.contenttitle}}</view>
        <view class='content'>{{item.content}}</view>
      </view>
  </view>
</view>
