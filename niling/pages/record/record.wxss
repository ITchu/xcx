.contentView {
  display: flex;
  padding: 10rpx 20rpx 20rpx 20rpx;
  flex-direction: row;
  align-items: center;
  border-bottom: 0.5px solid #eeeeee;
 
}

.titleview {
  width: 95%;
  padding-top: 10rpx;
  font-size: 34rpx;
  color: #333;
}

.timeview {
  padding-top:10rpx; 
  font-size: 30rpx;
  color: #666;
}

image {
  
  width: 16rpx;
  height: 30rpx;
}

.no-order{
    width: 100%;
    position: absolute;
    bottom: 0;
    top:0;
    left: 0;
    right: 0;
    text-align: center;
    padding-top: 40%;
    
}
.no-order-img{
    width: 142rpx;
    height: 160rpx;
  
}
.no-order .text{
    padding-top: 10rpx;
    font-size:30rpx;
    color:#999999;
    text-align: center
}
