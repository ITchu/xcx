.c_tabbar {
  border-top:1rpx solid rgb(238, 238, 238);
  width:100%;
  position: fixed;
  bottom:0;
  background:#F7F7FA;
  font-size:20rpx;
  /* color: rgb(255, 88, 133); */
  height: 88rpx;
  display: flex;
  padding-top:15rpx; 
  justify-content: space-around;
  align-items: center;
}

.c_tabbar .tabbar_item {
  text-align: center;
  overflow: hidden;
}

.c_tabbar .tabbar_item .tabbar_item_img {
  width:40rpx;
  height: 36rpx;
}
.c_tabbar .tabbar_item:nth-of-type(3) .tabbar_item_img{  
   width: 126rpx;
  height: 126rpx;
  position: absolute;
  top: -50%;
  margin-left: -63rpx;
  border-top: 2rpx solid #dcdcdc;
  border-radius: 50%; 
 } 
.c_tabbar .tabbar_item .tabbar_item_title.selected {
  color: rgb(255, 88, 133);
}
/*相机  */
.photo{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.xuan{
  width: 6rpx;
  height: 192rpx;
  background-color: rgb(0, 0, 0);
}
.xiangji{
  width: 266rpx;
  height: 180rpx;
  z-index: 2;  
}
.xiangji image{
  width: 100%;
  height: 100%;
  
}
.choose{
  width: 500rpx;
  height: 460rpx;
  border-radius: 30rpx;
  background-color: rgb(255, 255, 255);
  margin-top: -48rpx;
}
.choose .close{
  width: 100%;
  height: 44rpx;
  margin-bottom: 44rpx;
  margin-top: 22rpx;
  
}
.choose .close image{
  width: 44rpx;
  height: 44rpx;
  float: right;
  margin-right: 20rpx;
}
.choose .chooselist{
  width: 374rpx;
  margin: 0 auto;
}
.choose .chooselist image{
  width: 374rpx;
  height: 90rpx;
}
.choose .chooselist text{
  display: block;
  font-size: 28rpx;
  font-family: "PingFang";
  color: rgb(102, 102, 102);
  text-align: center;
  line-height: 2.5;
}
