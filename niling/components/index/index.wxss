/**index.wxss**/
.list{
  margin: 95rpx 26rpx 0 26rpx;
  display: flex;
  justify-content: space-around;
  align-content: space-around;
  flex-wrap: wrap;
  height: 480rpx;
  border-radius: 10rpx;
  background-color: rgb(255, 255, 255);
  box-shadow: 0rpx 1rpx 20rpx 0rpx rgba(51, 51, 51, 0.2);
}

.listDetail{
  width: 33.3%;
  text-align: center;
}
.listDetail image{
  display: block;
  width: 72rpx;
  height: 74rpx;
  margin: 0 auto;
}
.listDetail text{
  font-size: 30rpx;
  font-family: "PingFang";
  color: rgb(102, 102, 102);
  text-transform: uppercase;
  line-height: 2.5px;
  text-align: center; 
}

